{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "rrze/servicekatalog",
	"version": "1.3.0",
	"title": "Servicekatalog",
	"category": "rrze",
	"description": "Presentation of RRZE Services",
	"example": {},
	"attributes": {
		"selectedTargetGroups": {
			"type": "array",
			"default": {}
		},
		"selectedCommitments": {
			"type": "array",
			"default": {}
		},
		"selectedTags": {
			"type": "array",
			"default": {}
		},
		"selectedServices": {
			"type": "array",
			"default": {}
		},
		"numServices": {
			"type": "integer",
			"default": -1
		},
		"orderBy": {
			"type": "string",
			"default": ""
		},
		"selectedShowItems": {
			"type": "array",
			"default": ["thumbnail", "commitment", "group", "tag", "description", "url-portal", "url-description", "url-tutorial", "url-video", "urls"]
		},
		"layout": {
			"type": "string",
			"default": "grid"
		},
		"showSearchform": {
			"type": "boolean",
			"default": false
		},
		"showPdf": {
			"type": "boolean",
			"default": false
		},
		"showDisplaySwitcher": {
			"type": "boolean",
			"default": false
		},
		"teaserLength": {
			"type": "integer",
			"default": 50
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "rrze-servicekatalog",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}